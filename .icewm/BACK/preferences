# Status font
StatusFontName="-adobe-helvetica-bold-r-normal-*-17-*-*-*-p-*-*-1"
StatusFontNameXft="helvetica:size=10:bold"

# Number of milliseconds to display the workspace tooltip when changing workspaces
WorkspaceStatusTime=2500 # 0-2500

